<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <PackageId>TTNet.Data</PackageId>
    <Version>3.0.0</Version>
    <Authors>Eduardo Diez Vi√±uela</Authors>
    <Description>A .NET library for The Things Network Data API</Description>
    <RepositoryUrl>https://github.com/ed10vi/TTNet.Data</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <!--<PackageLicenseExpression>GPL-3.0-or-later</PackageLicenseExpression>-->
